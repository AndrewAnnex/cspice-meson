project('libcspice-test', 'c',
  meson_version: '>=0.63.0'
)
# get basic info
host = host_machine
system = host.system()
arch = host.cpu_family()
is_unix    = system == 'linux'
is_windows = system == 'windows'
is_macos   = system == 'darwin'
is_cygwin  = system == 'cygwin'
is_freebsd = system == 'freebsd'
is_arm     = arch == 'aarch64'
is_x86_64  = arch == 'x86_64'

# build the url
base_url = 'https://naif.jpl.nasa.gov/pub/naif/misc/toolkit_N0067/C/'
# get extension
if is_windows
  ext = 'cspice.zip'
else
  ext = 'cspice.tar.Z'
endif
# get the url
if is_macos and is_arm
  url = base_url + 'MacM1_OSX_clang_64bit/' + 'packages/cspice/' + ext
elif is_macos and is_x86_64
  url = base_url + 'MacIntel_OSX_AppleC_64bit/' + 'packages/cspice/' + ext
elif is_linux or is_freebsd
  url = base_url + 'PC_Linux_GCC_64bit/' + 'packages/cspice/' + ext
elif is_cygwin
  url = base_url + 'PC_Cygwin_GCC_64bit/' + 'packages/cspice/' + ext
else
  url = base_url + 'PC_Windows_VisualC_64bit/' + 'packages/cspice/' + ext
endif



# grab C compiler object
cc = meson.get_compiler('c')
# get paths
build_dir       = meson.current_build_dir()  
cspice_build   = join_paths(build_dir, 'cspice-src/')

# 1) Import FS to test for an existing directory
fs = import('fs')

# 2) If cspice-src/ isn't already there, fetch & unpack now
if not fs.is_dir(cspice_build)
  message('Fetching CSPICE sourcesâ€¦')
  cmd = run_command('bash', '-lc',
    # cd into builddir, download, strip the top folder, unpack
    'cd ' + build_dir + ' && ' +
    'curl -L ' + url + ' -o ' + ext + ' && ' +
    'mkdir -p cspice-src && ' +
    'gunzip -c cspice.tar.Z | tar xpf - --strip-components=1 -C cspice-src',
    check: true
  )
endif

# 2) Point Meson at the headers and sources (you still enumerate files)
inc = include_directories('cspice-src/include')
srcdir = join_paths(cspice_build, 'src', 'cspice') + '/'

message('SCRDIR: ' + srcdir)

cspice_srcs = files(
