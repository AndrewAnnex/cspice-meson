)

# locate math lib if you want a proper dependency object (optional)
m_dep = cc.find_library('m', required: false)

# get the windows def file
win_def = files('cspice.def')[0]

# 3) Build the shared library
cspice_lib = shared_library(
  'cspice',
  cspice_srcs,
  # compiler flags:
  c_args     : ['-std=c89', '-O3', '-fPIC', '-ansi', '-w'],
  dependencies : m_dep,
  include_directories: inc,
  soversion: '1',
  version: '67',
  darwin_versions: [67, 1],
  install: false,
  # only on Windows / MSVC:
  vs_module_defs : win_def 
)

# 4) Declare it as a Meson dependency for easy linking
cspice_dep = declare_dependency(
  include_directories: inc,
  link_with: cspice_lib
)
meson.override_dependency('cspice', cspice_dep)

# Build the test executable
test_exe = executable(
    'test_cspice', 
    'test_cspice.c',
    include_directories: inc,
    dependencies      : [ cspice_dep, m_dep ],
    install: false
)

test('cspice sanity', test_exe)

